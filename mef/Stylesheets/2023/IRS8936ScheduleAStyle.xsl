<?xml version="1.0" encoding="UTF-8" ?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<!-- CSS Styles for form 8936 Schedule A-->

  <xsl:template name="IRS8936ScheduleAStyle">
  
  .sty8936SchADotLn {
		letter-spacing:3.1mm;
		font-weight:bold;
		float:right;
		padding-right:2px;
	}
  
  .sty8936SchAVinBox {
		height:4mm;
		border-top-width:1px; 
		border-bottom-width:1px; 
		border-left-width:1px; 
		border-right-width:1px;
		border-style:solid; 
		border-color:black;
		width:90mm;
		clear:none;
		float:right;
		text-align:center;
	}
	
  .sty8936SchALine {
	    height:5mm;
	    border-bottom:1px solid black;
	    width:54mm;
	    padding-top:2px;
    }
  </xsl:template>

</xsl:stylesheet>
